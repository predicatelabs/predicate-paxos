{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "outDir": "tools/transactor/dist",
    "rootDir": "tools/transactor",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "tools/transactor/**/*.ts"
  ]
}